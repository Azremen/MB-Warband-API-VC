{
	"name": "Mount & Blade: Warband API",
	"publisher": "Shcherbyna, Revan & Sart",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.python",
				"match": "\\b(if|while|for|return|import|from|def|try|except)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.python",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.python",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.py"
}